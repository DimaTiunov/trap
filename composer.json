{
    "name": "buggregator/client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksei Gagarin (roxblnfk)",
            "homepage": "https://github.com/roxblnfk"
        },
        {
            "name": "Pavel Buchnev (butschster)",
            "homepage": "https://github.com/butschster"
        }
    ],
    "autoload": {
        "psr-4": {
            "Buggregator\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Buggregator\\Client\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.1",
        "ext-sockets": "*",
        "ext-zlib": "*",
        "psr/http-message": "^1",
        "clue/stream-filter": "^1.6",
        "php-http/message": "^1.15",
        "nyholm/psr7": "^1.8"
    },
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^10.2",
        "vimeo/psalm": "^5.11",
        "symfony/var-dumper": "^6",
        "symfony/process": "^6"
    }
}
