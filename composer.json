{
    "name": "buggregator/trap",
    "type": "library",
    "license": "BSD-3-Clause",
    "homepage": "https://buggregator.dev/",
    "authors": [
        {
            "name": "Aleksei Gagarin (roxblnfk)",
            "homepage": "https://github.com/roxblnfk"
        },
        {
            "name": "Pavel Buchnev (butschster)",
            "homepage": "https://github.com/butschster"
        }
    ],
    "autoload": {
        "psr-4": {
            "Buggregator\\Trap\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Buggregator\\Trap\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/trap"
    ],
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/buggregator"
        },
        {
            "type": "patreon",
            "url": "https://patreon.com/roxblnfk"
        },
        {
            "type": "patreon",
            "url": "https://patreon.com/butschster"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-sockets": "*",
        "clue/stream-filter": "^1.6",
        "nunomaduro/termwind": "^1.15",
        "nyholm/psr7": "^1.8",
        "php-http/message": "^1.15",
        "psr/http-message": "^1",
        "symfony/console": "^6.0",
        "symfony/var-dumper": "^6.0"
    },
    "require-dev": {
        "google/protobuf": "^3.23",
        "phpunit/phpunit": "^10.1",
        "symfony/process": "^6",
        "symfony/var-dumper": "^6",
        "vimeo/psalm": "^5.11"
    }
}
